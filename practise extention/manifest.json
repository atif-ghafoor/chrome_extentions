{
  "manifest_version": 2,
  "name": "Traing Extention",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "tx.png",
    "default_popup": "index.html",
    "default_title": "Change paragraph text"
  }
}
